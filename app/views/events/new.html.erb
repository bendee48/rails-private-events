<h1>Events</h1>
<%= form_with model: @event do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br />
  <%= f.label :description %><br />
  <%= f.text_area :body, size: '20x5' %><br />
  <%= f.label :date %>
  <%= f.datetime_select :date, order: [:day, :month, :year] %><br />
  <%= f.label :location %>
  <%= f.text_field :location %><br />
  <%= f.submit %>
<% end%>
